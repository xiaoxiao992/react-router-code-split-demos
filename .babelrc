{
    "presets": [
        ["env", {
            "modules": false,
            "targets": {
                "browsers": [
                    ">1%",
                    "last 4 versions",
                    "Firefox ESR",
                    "not ie < 9"
                ]
            }
        }],
        "react"
    ],
    "plugins": [
        "transform-object-rest-spread",

        "transform-decorators-legacy",
        "transform-decorators",
        "transform-class-properties",

        "transform-runtime",
        "syntax-dynamic-import",

        ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": true }]
    ]
}